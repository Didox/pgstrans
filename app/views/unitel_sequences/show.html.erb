<div class="container">

  <h4>Mostrar UnitelSequence</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Sequence:</dt>
      <dd><%= @unitel_sequence.sequence_id %></dd>
      <dt>Venda:</dt>
      <dd><%= @unitel_sequence.venda %></dd>
    </dl>

    <div class="page-header">
      <%= link_to unitel_sequences_path, class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_unitel_sequence_path(@unitel_sequence), class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
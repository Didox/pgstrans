<div class="container">
  <p id="notice"><%= notice %></p>

  <h4>UnitelSequence</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->

  <div class="container scroltable">
    <table class="table table-hover">
      <thead>
        <tr>
        <th scope="col">Sequence</th>
        <th scope="col">Venda</th>
        <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @unitel_sequences.each do |unitel_sequence| %>
        <tr>
          <td><%= unitel_sequence.sequence_id %></td>
          <td><a href="/vendas/<%= unitel_sequence.venda.id %>"><%= unitel_sequence.venda.id %></a></td>
          <td><%= link_to 'Mostrar', unitel_sequence %></td>
          <td><%= link_to 'Editar', edit_unitel_sequence_path(unitel_sequence) %></td>
          <td><%= link_to 'Apagar', unitel_sequence, method: :delete, data: { confirm: 'Tem certeza?' } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>

    <%= link_to "Novo Registo", new_unitel_sequence_path, class: 'btn btn-danger' %>
    <%= link_to 'Voltar', root_path, class: 'btn btn-danger' %>
  </div>
</div>
<div class="col-md-12 scroltable">
	<div class="col-md-12">
		<h4>Cadastro de Alegação de Pagamento</h4>
	</div>
	<!-- Page Heading/Breadcrumbs -->
	<%= render :partial => 'shared/header_backoffice' %>
	<!-- /.row -->

	<%= render 'form' %>

	<div class="col-md-12">
		<%= link_to alegacao_de_pagamentos_path, class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
			<span class="glyphicon glyphicon-transfer"></span>
			Voltar para o Índice
		<% end %>

		<%= link_to 'javascript:history.go(-1)', class: 'btn btn-info', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
			<span class="glyphicon glyphicon-arrow-left"></span>
			Voltar Anterior
		<% end %>
	</div>
</div>

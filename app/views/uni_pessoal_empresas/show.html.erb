<div class="col-md-12 scroltable">
	<div class="col-md-12">
    <h4>Mostrar Registo Cliente Unipessoal ou Empresa</h4>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  
  <div class="col-md-12">
    <dl class="dl-vertical">
      <dt>Nome:</dt>
      <dd><%= @uni_pessoal_empresa.nome %></dd>
    </dl>

    <div class="page-header">
      <%= link_to uni_pessoal_empresas_path, class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
        <span class="glyphicon glyphicon-arrow-left"></span>
        Voltar
      <% end %>
      <%= link_to edit_uni_pessoal_empresa_path(@uni_pessoal_empresa), class: 'btn btn-warning', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
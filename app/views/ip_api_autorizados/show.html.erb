<div class="container">

  <h4>Mostrar IpApiAutorizado</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Ip:</dt>
      <dd><%= @ip_api_autorizado.ip %></dd>
      <dt>Sub distribuidor:</dt>
      <dd><%= @ip_api_autorizado.sub_distribuidor.nome %></dd>
    </dl>

    <div class="page-header">
      <%= link_to ip_api_autorizados_path, class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_ip_api_autorizado_path(@ip_api_autorizado), class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
<div class="container">

  <p id="notice"><%= notice %></p>

  <h1>Return Code Apis</h1>

  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Return code</th>
        <th scope="col">Return description</th>
        <th scope="col">Error description</th>
        <th scope="col">Error description pt</th>
        <th scope="col">Partner Code</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @return_code_apis.each do |return_code_api| %>
        <tr>
          <td><%= return_code_api.return_code %></td>
          <td><%= return_code_api.return_description %></td>
          <td><%= return_code_api.error_description %></td>
          <td><%= return_code_api.error_description_pt %></td>
          <td><%= return_code_api.partner_code %></td>

          <td><%= link_to 'Show', return_code_api %></td>
          <td><%= link_to 'Edit', edit_return_code_api_path(return_code_api) %></td>
          <td><%= link_to 'Destroy', return_code_api, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Return Code Api', new_return_code_api_path %>
</div>
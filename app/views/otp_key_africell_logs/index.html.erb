<div class="col-md-12 scroltable">
	<div class="col-md-12">
    <h3>OTP_Key Africell Log</h3>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->
  <div class="col-md-12">
  <p id="notice"><%= notice %></p>
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col" style="width: 25%">Data</th>
          <th scope="col" style="width: 75%">Log</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @otp_key_africell_logs.each do |otp_key_africell_log| %>
        <tr>
          <td><%= otp_key_africell_log.data %></td>
          <td><%= otp_key_africell_log.log %></td>
        </tr>
        <% end %>
      </tbody>
    </table>

    <div class="apple_pagination">
      <%= will_paginate @otp_key_africell_logs, :container => false %>
    </div>
    <%= link_to 'Voltar', root_path, class: 'btn btn-primary' %>
  </div>
</div>
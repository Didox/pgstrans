<div class="col-md-12">
  <div class="col-md-12">
    <h4>Situação de Usuário / Cliente</h4>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>ID:</dt>
      <dd><%= @status_cliente.id %></dd>
      <dt>Nome:</dt>
      <dd><%= @status_cliente.nome %></dd>
    </dl>

    <div class="page-header">
      <%= link_to status_clientes_path, class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
        <span class="glyphicon glyphicon-arrow-left"></span>
        Voltar
      <% end %>
      <%= link_to edit_status_cliente_path(@status_cliente), class: 'btn btn-warning', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
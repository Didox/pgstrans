<div class="col-md-12 scroltable">
	<div class="col-md-12">
    <h3>Mostrar Situação de Alegação de Pagamento</h3>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>ID:</dt>
      <dd><%= @status_alegacao_de_pagamento.id %></dd>
      <dt>Nome:</dt>
      <dd><%= @status_alegacao_de_pagamento.nome %></dd>
    </dl>

    <div class="col-md-12">
      <%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_status_alegacao_de_pagamento_path(@status_alegacao_de_pagamento), class: 'btn btn-warning' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>

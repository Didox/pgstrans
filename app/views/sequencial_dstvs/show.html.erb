<div class="col-md-12 scroltable">
	<div class="col-md-12">
    <h3>Detalhes do Sequencial de Transação DSTV</h3>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Número do Transação:</dt>
      <dd><%= @sequencial_dstv.numero %></dd>
    </dl>

    <% if @adm.id == Usuario::ROOT %>
      <dl class="dl-horizontal">
        <dt>Envio:</dt>
        <dd><%= @sequencial_dstv.request_body %></dd>
      </dl>

      <dl class="dl-horizontal">
        <dt>Resposta:</dt>
        <dd><%= @sequencial_dstv.response_body %></dd>
      </dl>
    <% end %>

    <div class="page-header">
      <%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
        <span class="glyphicon glyphicon-arrow-left"></span>
        Voltar
      <% end %>
    </div>
  </div>
</div>
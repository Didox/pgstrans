<div class="container">

  <h4>Detalhes do Sequencial de Transação DSTV</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Número do Transação:</dt>
      <dd><%= @sequencial_dstv.numero %></dd>
    </dl>

    <dl class="dl-horizontal">
      <dt>Envio:</dt>
      <dd><%= @sequencial_dstv.request_body %></dd>
    </dl>

    <dl class="dl-horizontal">
      <dt>Resposta:</dt>
      <dd><%= @sequencial_dstv.response_body %></dd>
    </dl>

    <div class="page-header">
      <%= link_to sequencial_dstvs_path, class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
    </div>
  </div>
</div>
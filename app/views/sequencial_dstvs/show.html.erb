<div class="container">

  <h4>Mostrar SequencialDstv</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Numero:</dt>
      <dd><%= @sequencial_dstv.numero %></dd>
    </dl>

    <div class="page-header">
      <%= link_to sequencial_dstvs_path, class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_sequencial_dstv_path(@sequencial_dstv), class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
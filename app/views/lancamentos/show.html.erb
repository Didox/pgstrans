<div class="col-md-12 scroltable">
	<div class="col-md-12">
    <h4>Detalhes de Tipo de Lançamento</h4>
  </div>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->
  <dl class="dl-horizontal">
    <dt>ID:</dt>
    <dd><%= @lancamento.id %></dd>
    <dt>Nome:</dt>
    <dd><%= @lancamento.nome %></dd>
    <dt>Situação:</dt>
    <dd><%= @lancamento.status %></dd>    
  </dl>
  <div class="col-md-12">
    <%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
      <span class="glyphicon glyphicon-arrow-left"></span>
      Voltar
    <% end %>
    <%= link_to edit_lancamento_path(@lancamento), class: 'btn btn-warning', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
      <span class="glyphicon glyphicon-pencil"></span>
      Editar
    <% end %>
  </div>
</div>
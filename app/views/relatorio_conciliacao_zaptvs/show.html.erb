<div class="container">

  <h4>Mostrar RelatorioConciliacaoZaptv</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Parceiro:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.partner.name %></dd>
      <dt>URL de Integração:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.url %></dd>
      <dt>Operation Code (ID ZAP):</dt>
      <dd><%= @relatorio_conciliacao_zaptv.operation_code %></dd>
      <dt>Source Reference (ID Interno Pagasó):</dt>
      <dd><%= @relatorio_conciliacao_zaptv.source_reference %></dd>
      <dt>Código do Produto:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.product_code %></dd>
      <dt>Quantidade:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.quantity %></dd>
      <dt>Data e Hora:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.date_time %></dd>
      <dt>Tipo Transação:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.type_data %></dd>
      <dt>Valor Total:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.total_price %></dd>
      <dt>Status:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.status %></dd>
      <dt>Valor Unitário:</dt>
      <dd><%= @relatorio_conciliacao_zaptv.unit_price %></dd>
    </dl>

    <div class="page-header">
      <%= link_to "/partners", class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
    </div>
  </div>
</div>
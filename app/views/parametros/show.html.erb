<div class="container">

  <h4>Mostrar Parâmetros</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <div class="row bg-light h-50 p-3">
      <div class="col-md-4">
          Parceiro
      </div>
      <div class="col-md-8">

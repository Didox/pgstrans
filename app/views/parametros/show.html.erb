    <div class="row h-50 p-3">
      <div class="col-md-4">
          Data source DSTV desenvolvimento:
      </div>
      <div class="col-md-8">
          <%= @parametro.data_source_dstv_desenvolvimento %>
      </div>
    </div>

    <div class="row bg-light h-50 p-3">      
      <div class="col-md-4">
          Data source DSTV Produção:
      </div>
      <div class="col-md-8">
          <%= @parametro.data_source_dstv_producao %>
      </div>
    </div>

    <div class="row h-50 p-3">
      <div class="col-md-4">
          Payment vendor code DSTV desenvolvimento:
      </div>
      <div class="col-md-8">
          <%= @parametro.payment_vendor_code_dstv_desenvolvimento %>
      </div>
    </div>

    <div class="row bg-light h-50 p-3">
      <div class="col-md-4">
          Payment vendor code DSTV produção:
      </div>
      <div class="col-md-8">
          <%= @parametro.payment_vendor_code_dstv_producao %>
      </div>
    </div>
    
    <div class="row h-50 p-3">
      <div class="col-md-4">
          Vendor code DSTV desenvolvimento:
      </div>
      <div class="col-md-8">
          <%= @parametro.vendor_code_dstv_desenvolvimento %>

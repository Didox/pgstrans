<div class="col-md-12" data-target="toggle.grid">
  <div class="card border-simpleform box-shadow mb-4">
    <div class="card-header text-simpleform d-flex align-items-center justify-content-between">
      <div>
        <strong>Dados</strong><br />
      </div>
    </div>
  </div>

  <div class="card border-bootstrap box-shadow mb-4">
    <div class="card-header text-bootstrap align-items-center justify-content-between">
      
      <%= simple_form_for(@parametro, html: { class: "form-vertical", role: "form" }) do |f| %>
        <% if @parametro.errors.any? %>
          <div class="alert alert-danger alert-dismissable" role="alert">
            <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
            <h4><%= pluralize(@parametro.errors.count, "erros") %> no parametro, confira abaixo:</h4>

            <ul>
            <% @parametro.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
            </ul>
          </div>
        <% end %>

        <div class="form-input col-sm-6">
          <%= f.label :url_integracao_desenvolvimento, class: "col-sm-6 control-label" %>
          <%= f.text_field :url_integracao_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :url_integracao_producao, class: "col-sm-6 control-label" %>
          <%= f.text_field :url_integracao_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :partner, class: "col-sm-2 control-label" %>
          <%= f.text_field :partner, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :agent_key_movicel_desenvolvimento, class: "col-sm-6 control-label" %>
          <%= f.text_field :agent_key_movicel_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :agent_key_movicel_producao, class: "col-sm-6 control-label" %>
          <%= f.text_field :agent_key_movicel_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :user_id_movicel_desevolvimento, class: "col-sm-6 control-label" %>
          <%= f.text_field :user_id_movicel_desevolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :user_id_movicel_producao, class: "col-sm-6 control-label" %>
          <%= f.text_field :user_id_movicel_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :data_source_dstv_desevolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :data_source_dstv_desevolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :data_source_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :data_source_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :payment_vendor_code_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :payment_vendor_code_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :payment_vendor_code_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :payment_vendor_code_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :vendor_code_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :vendor_code_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :vendor_code_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :vendor_code_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :agent_account_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :agent_account_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :agent_account_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :agent_account_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :currency_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :currency_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :currency_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :currency_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :product_user_key_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :product_user_key_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :product_user_key_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :product_user_key_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :mop_dstv_desenvolvimento, class: "col-sm-2 control-label" %>
          <%= f.text_field :mop_dstv_desenvolvimento, class: "form-control" %>
        </div>
        <div class="form-input col-sm-6">
          <%= f.label :mop_dstv_producao, class: "col-sm-2 control-label" %>
          <%= f.text_field :mop_dstv_producao, class: "form-control" %>
        </div>
        <div class="form-actions col-sm-12">
          <%= f.button :button, class: 'btn btn-danger' do %>
            <span class="glyphicon glyphicon-ok"></span>
            Confirmar
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>

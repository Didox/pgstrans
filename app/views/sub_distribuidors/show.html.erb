<div class="col-md-12">
  <div class="col-md-12">
    <h4>Detalhe de Subdistribuidor</h4>
  </div>
  <br><br>
  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="col-md-12">
    <p>
      <strong>Nome:</strong>
      <%= @sub_distribuidor.nome %>
    </p>
    <p>
      <strong>BI:</strong>
      <%= @sub_distribuidor.bi %>
    </p>
    <p>
      <strong>Telefone:</strong>
      <%= @sub_distribuidor.telefone %>
    </p>
    <p>
      <strong>Morada:</strong>
      <%= @sub_distribuidor.morada %>
    </p>
    <p>
      <strong>Municipio:</strong>
      <%= @sub_distribuidor.municipio.nome %>
    </p>
    <p>
      <strong>Provincia:</strong>
      <%= @sub_distribuidor.provincia.nome %>
    </p>

    <div class="page-header">
      <%= link_to 'javascript:history.go(-1)', class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_sub_distribuidor_path(@sub_distribuidor), class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
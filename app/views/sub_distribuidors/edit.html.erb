<div class="col-md-12">
	<div class="col-md-12">
		<h4>Editando Distribuidor</h4>
	</div>
	<br><br>
	<%= render 'form', sub_distribuidor: @sub_distribuidor %>
</div>
<div class="col-md-12">
	<%= link_to @sub_distribuidor, class: 'btn btn-danger', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
		<span class="glyphicon glyphicon-info-sign"></span>
		Mostrar
	<% end %>
	<%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary', style: "width: 150px;text-align: center;margin-right: 0px;" do %>
		<span class="glyphicon glyphicon-arrow-left"></span>
		Voltar
	<% end %>
</div>
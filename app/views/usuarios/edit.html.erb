<div class="col-md-12"> 
  <div class="col-md-12"> 
    <h3>Edição de Dados de Usuários</h3>
  </div>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->

  <%= render 'form', usuario: @usuario %>

  <%= link_to @usuario, class: 'btn btn-danger' do %>
    <span class="glyphicon glyphicon-info-sign"></span>
    Mostrar
  <% end %>
  <%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
</div>
<div class="col-md-12">
	<div class="col-md-12">
		<h3>Cadastro de Novo Usuário</h3>
	</div>
	<!-- Page Heading/Breadcrumbs -->
  	<%= render :partial => 'shared/header_backoffice' %>
  	<!-- /.row -->

	<%= render 'form', usuario: @usuario %>

	<% if usuario_logado.agente? %>
    	<div class="form-group col-sm-12">
    	<label><strong>Após o cadastro, a equipa de Backoffice da Pagasó concluirá a parametrização e liberará o acesso!</strong></label>
      	<br>
    <% end %>

	<%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary' do %>
    	<span class="glyphicon glyphicon-arrow-left"></span>
    	Voltar
  	<% end %>

</div>
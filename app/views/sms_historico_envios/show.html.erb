<div class="container">

  <h4>Mostrar SmsHistoricoEnvio</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <dl class="dl-horizontal">
      <dt>Numero:</dt>
      <dd><%= @sms_historico_envio.numero %></dd>
      <dt>Conteudo:</dt>
      <dd><%= @sms_historico_envio.conteudo %></dd>
      <dt>Usuario:</dt>
      <dd><%= @sms_historico_envio.usuario %></dd>
      <dt>Venda:</dt>
      <dd><%= @sms_historico_envio.venda_id %></dd>
      <dt>Sucesso:</dt>
      <dd><%= @sms_historico_envio.sucesso %></dd>
    </dl>

    <div class="page-header">
      <%= link_to sms_historico_envios_path, class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
      <%= link_to edit_sms_historico_envio_path(@sms_historico_envio), class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
    </div>
  </div>
</div>
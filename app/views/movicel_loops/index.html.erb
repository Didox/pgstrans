<div class="container">
  <p id="notice"><%= notice %></p>

  <h4>MovicelLoop</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->

  <div class="container scroltable">
    <table class="table table-hover">
      <thead>
        <tr>
                    <th scope="col">Usuario</th>
                    <th scope="col">Token</th>
                    <th scope="col">Uri</th>
                    <th scope="col">Ambiente</th>
                    <th scope="col">Agente</th>
                    <th scope="col">Terminal</th>
                    <th scope="col">Valor</th>
                    <th scope="col">Repeticao</th>
                    <th scope="col">Nropedidoinicio</th>
                    <th scope="col">Nropedido</th>
                    <th scope="col">Request</th>
                    <th scope="col">Response</th>
                    <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @movicel_loops.each do |movicel_loop| %>
        <tr>
                    <td><%= movicel_loop.usuario %></td>
                    <td><%= movicel_loop.token %></td>
                    <td><%= movicel_loop.uri %></td>
                    <td><%= movicel_loop.ambiente %></td>
                    <td><%= movicel_loop.agente %></td>
                    <td><%= movicel_loop.terminal %></td>
                    <td><%= movicel_loop.valor %></td>
                    <td><%= movicel_loop.repeticao %></td>
                    <td><%= movicel_loop.nropedidoinicio %></td>
                    <td><%= movicel_loop.nropedido %></td>
                    <td><%= movicel_loop.request %></td>
                    <td><%= movicel_loop.response %></td>
                    <td><%= link_to 'Mostrar', movicel_loop %></td>
          <td><%= link_to 'Editar', edit_movicel_loop_path(movicel_loop) %></td>
          <td><%= link_to 'Apagar', movicel_loop, method: :delete, data: { confirm: 'Tem certeza?' } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>

    <%= link_to "Novo Registo", new_movicel_loop_path, class: 'btn btn-danger' %>
    <%= link_to 'Voltar', root_path, class: 'btn btn-danger' %>
  </div>
</div>
<div class="container">

  <h4>Mostrar LoopLog</h4>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->  

  <div class="container">
    <h1>Envio para movicel</h1>
    <%= raw @loop_log.request %>
    <hr>
    <br>
    <br>
    <br>
    
    <h1>Resposta da movicel</h1>
    <%= raw @loop_log.response %>

    <div class="page-header">
      <%= link_to "/movicel_loops/#{@loop_log.movicel_loop_id}/loop_logs", class: 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Voltar
      <% end %>
    </div>
  </div>
</div>
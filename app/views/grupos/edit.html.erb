<div class="col-sm-12">

  <div class="col-sm-12">
    <h3>Edição Grupo</h3>
  </div>

  <!-- Page Heading/Breadcrumbs -->
  <%= render :partial => 'shared/header_backoffice' %>
  <!-- /.row -->

  <%= render 'form' %>

  <div class="col-sm-12">
    <%= link_to @grupo, class: 'btn btn-danger' do %>
      <span class="glyphicon glyphicon-info-sign"></span>
      Mostrar
    <% end %>
    <%= link_to 'javascript:history.go(-1)', class: 'btn btn-primary' do %>
      <span class="glyphicon glyphicon-list-alt"></span>
      Voltar
    <% end %>
  </div>
</div>